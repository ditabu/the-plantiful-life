<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Plants List</title>
</head>
<body>
    <table id="plantList">
        <thead>
          <tr>
            <th>Plant Name</th>
            <th>Plant<br>Species</th>
            <th>Light</th>
            <th>Water</th>
            <th>Groom</th>
            <th>Climate</th>
          </tr>
        </thead>
        <tbody>
          <% plants.forEach(function(p) { %>
            <tr>
              <td><%= p.plantName %></td>
              <td><%= p.plantSpecies %></td>
              <td><%= p.light %></td>
              <td><%= p.water %></td>
              <td><%= p.groom %></td>
              <td><%= p.climate %></td>
            </tr>
            <% }); %>
          </tbody>
        </table>        
</body>
</html>